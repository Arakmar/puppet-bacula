NameVirtualHost <%= ipaddress %>:<%= port %>
<VirtualHost <%= ipaddress %>:<%= port %>>
  servername <%= name %>
  ServerAdmin webmaster@puppetlabs.com
  DocumentRoot <%= vhost_dir %>
  UseCanonicalName On
  <Directory />
   Options FollowSymLinks
    AllowOverride ALL
    AuthType Digest
  </Directory>
  <Directory <%= vhost_dir %>>
    Options Indexes FollowSymLinks MultiViews
    AllowOverride All
    Order allow,deny
    HeaderName IndexHeader.html
    allow from all
    AuthType basic
    AuthName "private area"
    AuthBasicProvider file
    AuthUserFile <%=docroot%>/.htpasswd
    Require valid-user
  </Directory>
  ErrorLog /var/log/apache2/puppetlabs.com-error.log
  # Possible values include: debug, info, notice, warn, error, crit,
  # alert, emerg.
  LogLevel debug
  CustomLog /var/log/apache2/puppetlabs.com.access.log combined
  RewriteEngine On
  RewriteCond %{HTTP_HOST} ^reductivelabs\.com$
  RewriteRule ^(.*)$ http://www.puppetlabs.com%{REQUEST_URI} [R=permanent,L]
  Redirect permanent /category/news http://puppetlabs.com/category/blog/general-news/
  Redirect permanent /category/puppet http://puppetlabs.com/category/blog
  RedirectMatch permanent ^/category/downloads$ http://puppetlabs.com/resources/downloads
  RedirectMatch permanent ^/download$ http://puppetlabs.com/resources/downloads
  Redirect permanent /home/about/puppet-mailing-list http://puppetlabs.com/company/#contact
  Redirect permanent /home/about http://puppetlabs.com/company
  Redirect permanent /home/irc http://www.puppetlabs.com/blog 
  Redirect permanent /jobs http://www.puppetlabs.com/company 
  RedirectMatch permanent ^/podcast(.*)$ http://puppetlabs.com/blog
  Redirect permanent /products/feed http://www.puppetlabs.com/blog
  Redirect permanent /products/home/users http://www.puppetlabs.com/blog
  Redirect permanent /products/home http://www.puppetlabs.com
  Redirect permanent /products/facter http://www.puppetlabs.com/puppet/related-projects/facter/
  Redirect permanent /products/puppet http://www.puppetlabs.com
  Redirect permanent /products http://www.puppetlabs.com/puppet
  Redirect permanent /services/feed http://www.puppetlabs.com/blog
  Redirect permanent /services/subscription-support http://www.puppetlabs.com/services/support
  Redirect permanent /tag/puppet http://www.puppetlabs.com/blog
