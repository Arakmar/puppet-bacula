title       "Subproject issues over the past 6 months"
vtitle      "Redmine tickets"
from        "-6months"
description "Open issues for each given subproject over the past 6 months."
linewidth   2
ymin        0

field :dashboard,
               :color => "aqua",
               :alias => "Dashboard issues (total)",
               :data  => "redmine.dashboard.issues.count"

field :facter,
               :color => "green",
               :alias => "Facter issues (total)",
               :data  => "redmine.facter.issues.count"

field :modulesite,
               :color => "yellow",
               :alias => "Module site issues (total)",
               :data  => "redmine.module-site.issues.count"
