title       "Subproject issues over the past month"
vtitle      "Redmine tickets"
from        "-1month"
description "Open issues for each given subproject over the past month."
linewidth   2
ymin        0

field :dashboard,
               :color => "aqua",
               :alias => "Dashboard issues (total)",
               :data  => "redmine.dashboard.issues.open"

field :facter,
               :color => "green",
               :alias => "Facter issues (total)",
               :data  => "redmine.facter.issues.open"

field :modulesite,
               :color => "yellow",
               :alias => "Module site issues (total)",
               :data  => "redmine.module-site.issues.open"
