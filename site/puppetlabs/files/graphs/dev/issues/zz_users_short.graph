title       "New account registrations"
vtitle      "New user registrations"
from        "-6months"
description "New users registered per day in Redmine."
linewidth   2
ymin        0

field :dashboard,
               :color => "orange",
               :alias => "New account registrations (daily)",
               :data  => 'derivative(summarize(redmine.users,"1day","avg"))'
