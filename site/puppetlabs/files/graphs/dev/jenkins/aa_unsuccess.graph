title       "Currently unsuccessful tests by project"
vtitle      "Failures"
from        "-1week"
description "Number of currently unsuccessful Jenkins tests in each project."
linewidth   2
ymin        0

field :puppet,
               :alias => "Currently unsuccessful Jenkins tests in Puppet",
							 :data  => "jenkins.puppet.currently-unsuccessful"

field :facter,
               :alias => "Currently unsuccessful Jenkins tests in Facter",
               :data  => "jenkins.facter.currently-unsuccessful"
							 
field :marionette-collective,
               :alias => "Currently unsuccessful Jenkins tests in MCollective",
               :data  => "jenkins.marionette-collective.currently-unsuccessful"

field :pe-12,
               :alias => "Currently unsuccessful Jenkins tests in PE 1.2",
							 :data  => "jenkins.puppet.currently-unsuccessful"

field :pe-20,
               :alias => "Currently unsuccessful Jenkins tests in PE 2.0",
							 :data  => "jenkins.puppet.currently-unsuccessful"
							 
field :puppet-dashboard,
               :alias => "Currently unsuccessful Jenkins tests in Puppet",
							 :data  => "jenkins.puppet-dashboard.currently-unsuccessful"
