               :alias => "Currently unsuccessful Jenkins tests in PE 2.0",
               :data  => "jenkins.puppet.currently-unsuccessful"

field :marionettecollective,
               :alias => "Currently unsuccessful Jenkins tests in MCollective",
               :data  => "jenkins.marionette-collective.currently-unsuccessful"

