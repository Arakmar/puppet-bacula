FileSet {
  Name = <%= name %>
  Include {
    Options {
      signature = MD5
      compression=gzip
    }
<% if files.is_a? Array %>
<% files.each do |f| %><%= "    File = #{f}\n" %><% end %>
<% elsif files != '' %>
<%= "    File = #{f}" %>
<% end %>
  }
  Exclude {
<% if files.is_a? Array %>
<% excludes.each do |exclude| %><%= "    File = #{exclude}\n" %><% end %>
<% elsif excludes != '' %>
<%= "    File = #{exclude}" %>
<% end %>
  }
}
