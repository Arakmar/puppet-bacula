#
# Bacula User Agent (or Console) Configuration File
#

Director {
  Name     = <%= scope.lookupvar('bacula::params::director_name') %>-dir
  DIRport  = <%= @port %>
  address  = <%= scope.lookupvar('bacula::params::director_address') %>
  Password = "<%= @password %>"
  <%= scope.function_template(['bacula/_ssl.erb']) %>
}

