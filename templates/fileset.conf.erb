FileSet {
  Name = <%= @name %>
  Include {
    Options {
      signature   = MD5
      compression = GZIP
    }
<% [@files].flatten.each do |f| %>
<%=    "    File = #{f}\n" %>
<% end -%>
  }
  Exclude {
<%   [@excludes].flatten.reject { |x| x.empty? }.flatten.each do |exclude| %>
<%=    "    File = #{exclude}\n" %>
<% end -%>
  }
}

