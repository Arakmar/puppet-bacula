Client {
  Name           = <%= fqdn %>-fd
  Address        = <%= fqdn %>
  FDPort         = <%= scope.lookupvar('bacula::params::file_port') %>
  Catalog        = MyCatalog
  Password       = "<%= scope.lookupvar('bacula::params::bacula_password') %>"
  File Retention = <%= file_retention %>
  Job Retention  = <%= job_retention %>
  AutoPrune      = <%= autoprune %>
}

